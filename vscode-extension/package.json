{
  "name": "react-native-sztudio",
  "displayName": "react-native-sztudio",
  "description": "A sztudio.",
  "activationEvents": [
    "workspaceContains:node_modules/react-native"
  ],
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.75.0"
  },
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "RNStudio.showPreviewsPanel",
        "title": "RNStudio: Open Previews"
      },
      {
        "command": "RNStudio.reloadMetro",
        "title": "RNStudio: Reload Metro"
      },
      {
        "command": "RNStudio.startDebuggingSession",
        "title": "RNStudio: Start Debugger"
      }
    ],
    "breakpoints": [
      {
        "language": "typescript"
      },
      {
        "language": "javascript"
      }
    ],
    "debuggers": [
      {
        "type": "com.swmansion.react-native-preview",
        "label": "React Native Preview",
        "languages": [
          "javascript",
          "typescript"
        ]
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run package",
    "build:extension-debug": "webpack",
    "build:sim-controller-debug": "npm run build:sim-controller -- Debug",
    "build:sim-controller": "./scripts/build-sim-controller.sh",
    "build:debug": "npm run build:extension-debug && npm run build:sim-controller-debug",
    "package": "npm run build-sim-controller && webpack --mode production --devtool hidden-source-map",
    "start:webview": "webpack serve --mode development",
    "lint": "eslint src --ext ts"
  },
  "devDependencies": {
    "@babel/preset-react": "^7.23.3",
    "@types/glob": "^7.1.3",
    "@types/node": "^16.18.65",
    "@types/node-fetch": "^2.6.9",
    "@types/vscode": "^1.46.0",
    "@types/ws": "^8.5.10",
    "@types/xml2js": "^0.4.14",
    "@typescript-eslint/eslint-plugin": "^4.14.1",
    "@typescript-eslint/parser": "^4.14.1",
    "@vscode/vsce": "^2.22.0",
    "babel-loader": "^9.1.3",
    "eslint": "^7.19.0",
    "file-loader": "^6.2.0",
    "glob": "^7.1.6",
    "prettier": "^2.2.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "string-replace-webpack-plugin": "^0.1.3",
    "ts-loader": "^9.5.1",
    "typescript": "^4.1.3",
    "url-loader": "^4.1.1",
    "vscode-test": "^1.5.0",
    "webpack-cli": "^5.1.4"
  },
  "dependencies": {
    "@react-native-community/cli": "^11.3.7",
    "@vscode/codicons": "^0.0.35",
    "@vscode/debugadapter": "^1.51.0",
    "@vscode/debugprotocol": "^1.51.0",
    "@vscode/webview-ui-toolkit": "^1.3.1",
    "chrome-remote-interface": "^0.33.0",
    "execa": "^5.1.1",
    "node-fetch": "^2.7.0",
    "source-map": "^0.8.0-beta.0",
    "ws": "^8.14.2",
    "xdl": "^60.0.10",
    "xml2js": "^0.6.2"
  }
}
