{
  "name": "react-native-sztudio",
  "displayName": "react-native-sztudio",
  "description": "A sztudio.",
  "activationEvents": [
    "*"
  ],
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.75.0"
  },
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "RNStudio.showPreviewsPanel",
        "title": "RNStudio: Open Previews"
      },
      {
        "command": "RNStudio.reloadMetro",
        "title": "RNStudio: Reload Metro"
      },
      {
        "command": "RNStudio.startDebuggingSession",
        "title": "RNStudio: Start Debugger"
      }
    ],
    "breakpoints": [
      {
        "language": "typescript"
      },
      {
        "language": "javascript"
      }
    ],
    "debuggers": [
      {
        "type": "com.swmansion.react-native-preview",
        "label": "React Native Preview",
        "languages": [
          "javascript",
          "typescript"
        ]
      }
    ]
  },
  "scripts": {
    "install:all": "npm install && cd webview-ui && npm install",
    "build:all": "cd webview-ui && npm run build",
    "start:webview": "cd webview-ui && npm run serve",
    "build:webview": "cd webview-ui && npm run build",
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src --ext ts"
  },
  "devDependencies": {
    "@types/glob": "^7.1.3",
    "@types/node": "^16.18.65",
    "@types/vscode": "^1.46.0",
    "@types/ws": "^8.5.10",
    "@types/xml2js": "^0.4.14",
    "@typescript-eslint/eslint-plugin": "^4.14.1",
    "@typescript-eslint/parser": "^4.14.1",
    "eslint": "^7.19.0",
    "glob": "^7.1.6",
    "prettier": "^2.2.1",
    "typescript": "^4.1.3",
    "vscode-test": "^1.5.0"
  },
  "dependencies": {
    "@react-native-community/cli": "^11.3.7",
    "@vscode/debugadapter": "^1.51.0",
    "@vscode/debugprotocol": "^1.51.0",
    "chrome-remote-interface": "^0.33.0",
    "execa": "^5.1.1",
    "node-fetch": "^2.7.0",
    "source-map": "^0.7.4",
    "ws": "^8.14.2",
    "xdl": "^60.0.10",
    "xml2js": "^0.6.2"
  }
}
